            \r\n                        <v-spacer></v-spacer>\r\n                         <v-dialog v-model=\"dialog1\" max-width=\"500px\">\r\n                            <template v-slot:activator=\"{ on, attrs }\">\r\n                                <v-btn color=\"success\" dark class=\"mb-2\" v-bind=\"attrs\" v-on=\"on\">\r\n                                    <v-icon small class=\"mr-2\">\r\n                                        mdi-plus-box-multiple-outline\r\n                                    </v-icon> Choose Investment Type\r\n                                </v-btn>\r\n                            </template>\r\n                            <v-card>\r\n                                <div class=\"title pt-4\">\r\n                                    <span class=\"text-h6\">Choose Investment Type </span>\r\n                                </div>\r\n\r\n                                <v-card-text>\r\n                                    <v-container>\r\n                                        <v-row>\r\n\r\n                                           <v-col cols=\"12\">\r\n                                                <b-form-select v-model=\"editedItem.investment_type\" class=\"select p-2 \" style=\"width:100%; background:#fff;\" :options=\"options\"></b-form-select>\r\n                                            </v-col>\r\n                                           \r\n                                            <v-col cols=\"12\">\r\n                                                <b-button v-b-modal.modal-multi-1 style=\"background: #20618c; color: #fff; width: 100%\" @click=\"saveInvestmenttype\" size=\"lg\">Submit</b-button>\r\n                                            </v-col>\r\n\r\n                                        </v-row>\r\n                                    </v-container>\r\n                                </v-card-text>\r\n\r\n                            </v-card>\r\n                        </v-dialog>\r\n                        <v-spacer></v-spacer>\r\n                        <v-dialog v-model=\"dialog\" max-width=\"500px\">\r\n                            <template v-slot:activator=\"{ on, attrs }\">\r\n                                <v-btn color=\"primary\" dark class=\"mb-2\" v-bind=\"attrs\" v-on=\"on\">\r\n                                    <v-icon small class=\"mr-2\">\r\n                                        mdi-plus-box-multiple-outline\r\n                                    </v-icon> Make Investment\r\n                                </v-btn>\r\n                            </template>\r\n                            <v-card>\r\n                                <div class=\"title pt-4\">\r\n                                    <span class=\"text-h6\">Make Investment </span>\r\n                                </div>\r\n\r\n                                <v-card-text>\r\n                                    <v-container>\r\n                                        <v-row>\r\n\r\n                                            <v-col cols=\"12\">\r\n                                                <b-form-input v-model=\"editedItem.investment_amount\" placeholder=\"Amount\"></b-form-input>\r\n\r\n                                            </v-col>\r\n                                           \r\n                                            <v-col cols=\"12\">\r\n                                                <b-button v-b-modal.modal-multi-1 style=\"background: #20618c; color: #fff; width: 100%\" @click=\"save\" size=\"lg\">Invest</b-button>\r\n                                            </v-col>\r\n\r\n                                        </v-row>\r\n                                    </v-container>\r\n                                </v-card-text>\r\n\r\n                            </v-card>\r\n                        </v-dialog>\r\n                        <v-dialog v-model=\"dialogDelete\" max-width=\"500px\">\r\n                            <v-card>\r\n                                <v-card-title class=\"text-h5\">Are you sure you want to delete this item?</v-card-title>\r\n                                <v-card-actions>\r\n                                    <v-spacer></v-spacer>\r\n                                    <v-btn color=\"blue darken-1\" text @click=\"closeDelete\">Cancel</v-btn>\r\n                                    <v-btn color=\"blue darken-1\" text @click=\"deleteItemConfirm\">OK</v-btn>\r\n                                    <v-spacer></v-spacer>\r\n                                </v-card-actions>\r\n                            </v-card>\r\n                        </v-dialog>\r\n                    </v-toolbar>\r\n                </template>\r\n\r\n                <template v-slot:item.investment_amount=\"{item}\">\r\n                <div>\r\n                    {{ formatMoney(item.investment_amount) }}\r\n                </div>\r\n                </template>\r\n\r\n                <template v-slot:item.min_amount=\"{item}\">\r\n                <div>\r\n                    {{ formatMoney(item.min_amount) }}\r\n                </div>\r\n                </template>\r\n\r\n                <template v-slot:item.actions=\"{ item }\">\r\n                    <v-icon small class=\"mr-2\">\r\n                        mdi-dots-horizontal\r\n                    </v-icon>\r\n                </template>\r\n                <template v-slot:no-data>\r\n                    <v-btn color=\"primary\" @click=\"initialize\">\r\n                        Reset\r\n                    </v-btn>\r\n                </template>\r\n            </v-data-table>\r\n\r\n        </div>\r\n        <div>\r\n            <div>\r\n                <b-table striped hover :items=\"items\"></b-table>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</b-container>\r\n</template>\r\n\r\n<script>\r\nimport swal from 'sweetalert2';\r\nexport default {\r\n    data: () => ({\r\n        dialog: false,\r\n        dialog1: false,\r\n        dialogDelete: false,\r\n        \r\n        options: [\r\n\r\n         ],      \r\n     \r\n        headers: [{\r\n                text: 'S/N',\r\n                align: 'start',\r\n                sortable: false,\r\n                value: 'sn',\r\n                class: \"blue lighten-5\"\r\n            },\r\n            {\r\n                text: 'Phases Name',\r\n                value: 'phases',\r\n                class: \"blue lighten-5\"\r\n            },\r\n            {\r\n                text: 'Amount',\r\n                value: 'investment_amount',\r\n                class: \"blue lighten-5\"\r\n            },\r\n            {\r\n                text: 'Investment Type(Periods)',\r\n                value: 'investment_id',\r\n                class: \"blue lighten-5\"\r\n            },\r\n            \r\n            {\r\n                value: 'id',\r\n                align: ' d-none'\r\n                \r\n            },\r\n            {\r\n                text: '',\r\n                value: 'edit',\r\n                sortable: false,\r\n                class: \"blue lighten-5\"\r\n            },\r\n        ],\r\n        sponsor_n: '',\r\n        desserts: [],\r\n        editedIndex: -1,\r\n        editedItem: {\r\n            sn: [],\r\n            phases: '',\r\n            investment_amount: '',\r\n            investment_id: '',\r\n            //investment_type: '',\r\n            edit: '...',\r\n        },\r\n        defaultItem: {\r\n            sn: [],\r\n            phases: '',\r\n            investment_amount: '',\r\n            investment_id: '',\r\n            //investment_type: '',\r\n            edit: '',\r\n        },\r\n    }),\r\n    beforeMount(){\r\n    const list = this.find();\r\n    const findInvestment = this.findInvestment();\r\n  \r\n\r\n    },\r\n    computed: {\r\n        indexSn() {\r\n            return this.desserts.map((d, index) => ({\r\n                ...d,\r\n                sn: index + 1\r\n            }))\r\n        },\r\n    },\r\n\r\n    watch: {\r\n        dialog(val) {\r\n            val || this.close()\r\n        },\r\n\r\n        dialog1(val) {\r\n            val || this.close()\r\n        },\r\n\r\n        dialogDelete(val) {\r\n            val || this.closeDelete()\r\n        },\r\n    },\r\n\r\n    methods: {\r\n\r\n        formatMoney(num) {\r\n        return Number(num).toLocaleString(\"en-US\", {\r\n            style: \"currency\",\r\n            currency: \"ngn\"\r\n        });\r\n        },\r\n        \r\n        initialize() {\r\n            this.desserts = [\r\n            ]\r\n        },\r\n\r\n        close() {\r\n            this.dialog = false\r\n            this.$nextTick(() => {\r\n                this.editedItem = Object.assign({}, this.defaultItem)\r\n                this.editedIndex = -1\r\n            })\r\n        },\r\n\r\n         close() {\r\n            this.dialog1 = false\r\n            this.$nextTick(() => {\r\n                this.editedItem = Object.assign({}, this.defaultItem)\r\n                this.editedIndex = -1\r\n            })\r\n        },\r\n\r\n        closeDelete() {\r\n            this.dialogDelete = false\r\n            this.$nextTick(() => {\r\n                this.editedItem = Object.assign({}, this.defaultItem)\r\n                this.editedIndex = -1\r\n            })\r\n        },\r\n        //   async toogleStatus(id){\r\n        //     try {\r\n        //         const url = \"/scheme-status\";\r\n        //         const resp = await this.$http.post(url, {\r\n        //             id\r\n        //         })\r\n        //         console.log({id})\r\n        //         // TODO check if resp is successful\r\n\r\n        //         // change the status from the front\r\n        //          if (resp.data.code == '00') {\r\n        //             const success_message = resp.data.message;\r\n        //               const success_status = resp.data.status;\r\n        //             swal.fire({\r\n        //                 icon: 'success',\r\n        //       